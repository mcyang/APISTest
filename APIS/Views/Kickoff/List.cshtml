@model APIS.ViewModels.KickoffListViewModel

@{
    ViewBag.Title = "List";
}

<ol class="breadcrumb">
    <li><a href="#">MKT管理</a></li>
    <li><a href="#">開案單維護</a></li>
    <li class="active">列表頁</li>
</ol>

<h2>開案單維護</h2>

<div>
    <table class="table table-hover">
        <tr>
            <th>開案單編號</th>
            <th>專案名稱</th>
            <th>管理</th>
        </tr>
        @if (Model.ID != 0)
        {
            <tr>
                <td>@Model.RDRNumber</td>
                <td>@Model.ProjectName</td>
                <td>
                    @Html.ActionLink("列印", "Print", new { id = Model.ID })
                    @*@Html.ActionLink("Edit", "Edit", new { id = Model.ID }) |
                    @Html.ActionLink("Details", "Details", new { id = Model.ID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = Model.ID })*@
                </td>
            </tr>
        }
    </table>
</div>
